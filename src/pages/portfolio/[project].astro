---
import Layout from "../../layouts/Layout.astro";

export function getStaticPaths() {
  return [
    { params: { project: "sofa" } },
    { params: { project: "keyboard" } },
    { params: { project: "work-media" } },
  ];
}

const { project } = Astro.params;
---

<Layout title="Portfolio">
  <h1>{project}</h1>
</Layout>
